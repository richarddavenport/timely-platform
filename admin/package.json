{
  "name": "product-development.web",
  "version": "0.0.1",
  "scripts": {
    "svelte-kit-dev": "svelte-kit dev",
    "dev": "run-p svelte-kit-dev watch",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "next": "npm i @sveltejs/kit@next",
    "generate": "npx graphql-codegen --config codegen.yml",
    "watch": "cross-env SCHEMA_ENDPOINT=http://localhost:8080/v1/graphql nodemon --exec \"npm run generate\" --watch ./src/lib/queries -e graphql",
    "start": "node ./build/index.js"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/introspection": "^2.0.0",
    "@graphql-codegen/typed-document-node": "^2.0.0",
    "@graphql-codegen/typescript": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^2.0.1",
    "@sveltejs/adapter-node": "^1.0.0-next.55",
    "@sveltejs/kit": "^1.0.0-next.188",
    "@types/cookie": "^0.4.1",
    "@types/lodash": "^4.14.172",
    "@types/node-fetch": "^2.5.12",
    "@types/qs": "^6.9.7",
    "@types/toastify-js": "^1.9.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "cross-env": "^7.0.3",
    "date-picker-svelte": "^1.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.37.0",
    "simple-svelte-autocomplete": "^2.2.3",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite-plugin-windicss": "1.2.7",
    "windicss": "3.1.7"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.2.2",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@lukeed/uuid": "^2.0.0",
    "@popperjs/core": "^2.10.2",
    "@rgossiaux/svelte-headlessui": "^1.0.0-beta.7",
    "collect.js": "^4.29.0",
    "cookie": "^0.4.1",
    "dayjs": "^1.10.7",
    "graphql": "^15.6.0",
    "graphql-tag": "^2.12.5",
    "lodash": "^4.17.21",
    "logrocket": "^2.0.0",
    "modern-normalize": "^1.1.0",
    "node-fetch": "^2.6.1",
    "qs": "^6.10.1",
    "svelte-portal": "^2.1.2"
  }
}
